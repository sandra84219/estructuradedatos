#include "stdafx.h"
#include <iostream>
#include <conio.h>
using namespace std;
int main () {
	int a,b,c,n;
	cout<<"ingrese un numero"<<endl;
	cin>>n;
	while (n>0) {
		c=n%10;
		n=n/10;
	}
	if (c<8) {
			cout<<"es complemento"<<endl;
		}else{
			cout<<"no es complemento"<<endl;
	}
	getch();
	return (0);
}
