#include "stdafx.h"
#include <iostream>
#include <conio.h>
using namespace std;
int main () {
	int n,a,b,t,c;
	cout<<"ingrese un numero"<<endl;
	cin>>n;
	b=0;
	t=1;
	while(n>0) {
		a=n%10;
		c=9-a;
		b=(c*t)+b;
		t=t*10;
		n=n/10;
	}
	cout<<"el complemento es"<<endl;
	cout<<b;
	getch();
	return (0);
}
